{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type":"object",
    "properties": {
        "success": {
            "type": "boolean"
        },
        "links": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "icon": {
                        "type": "string"
                    },
                    "link": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "icon",
                    "link"
                ]
            }
        }
    },
    "required": [
        "success",
        "links"
    ]
}